    targetNamespace="http://claude-family/bpmn/L1-stubs"
    id="Definitions_L1_Stubs">

  <!--
    L1 Stub Processes for L0 Call Activity Resolution

    Minimal stub processes (startEvent → userTask → endEvent) used by
    L0_claude_family.bpmn to resolve callActivity references without loading
    the full L1 implementations. Each stub has a single user task that
    acts as a placeholder for the real L1 process.

    Stubs provided for:
      - 6 existing capabilities (session, work tracking, knowledge, enforcement, agents, config)
      - 2 new architecture models (core claude, claude family extensions)
  -->

  <!-- Existing L1 capabilities -->

  <bpmn:process id="L1_session_management" name="L1 Stub: Session Management" isExecutable="true">
    <bpmn:startEvent id="sm_stub_start" name="Start">
      <bpmn:outgoing>sm_f1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="sm_placeholder" name="Session Management Placeholder">
      <bpmn:incoming>sm_f1</bpmn:incoming>
      <bpmn:outgoing>sm_f2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="sm_stub_end" name="End">
      <bpmn:incoming>sm_f2</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="sm_f1" sourceRef="sm_stub_start" targetRef="sm_placeholder"/>
    <bpmn:sequenceFlow id="sm_f2" sourceRef="sm_placeholder" targetRef="sm_stub_end"/>
  </bpmn:process>

