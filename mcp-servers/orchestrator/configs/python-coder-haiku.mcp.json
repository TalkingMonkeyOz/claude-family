{
  "mcpServers": {
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:\\Projects"
      ],
      "env": {}
    },
    "postgres": {
      "type": "stdio",
      "command": "C:\\venvs\\mcp\\Scripts\\postgres-mcp.exe",
      "args": [
        "--access-mode=unrestricted"
      ],
      "env": {
        "DATABASE_URI": "postgresql://postgres:postgres@localhost/ai_company_foundation"
      }
    },
    "python-repl": {
      "type": "stdio",
      "command": "C:\\venvs\\mcp\\Scripts\\mcp-python.exe",
      "args": [],
      "env": {}
    },
    "orchestrator": {
      "type": "stdio",
      "command": "C:\\venvs\\mcp\\Scripts\\python.exe",
      "args": ["C:\\Projects\\claude-family\\mcp-servers\\orchestrator\\server.py"],
      "cwd": "C:\\Projects\\claude-family\\mcp-servers\\orchestrator",
      "env": {
        "DATABASE_URI": "postgresql://postgres:postgres@localhost:5432/ai_company_foundation"
      }
    }
  }
}
